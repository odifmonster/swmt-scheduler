import datetime
from app.support import HasID
from app.style.color import ShadeGrade
from app.schedule.dyelot import DyeLot

class Job(HasID[int]):
    def __init__(self, start: datetime.datetime,
                 cycle_time: datetime.timedelta,
                 max_date: datetime.datetime,
                 lots: tuple[DyeLot, ...] = ...) -> None: ...
    @property
    def start(self) -> datetime.datetime: ...
    @start.setter
    def start(self, new: datetime.datetime) -> None: ...
    @property
    def cycle_time(self) -> datetime.timedelta: ...
    @property
    def end(self) -> datetime.datetime: ...
    @property
    def before_date(self) -> datetime.datetime: ...
    @property
    def lbs(self) -> float: ...
    @property
    def yds(self) -> float: ...
    @property
    def lots(self) -> tuple[DyeLot, ...]: ...
    @property
    def shade(self) -> ShadeGrade: ...
    def __repr__(self) -> str: ...